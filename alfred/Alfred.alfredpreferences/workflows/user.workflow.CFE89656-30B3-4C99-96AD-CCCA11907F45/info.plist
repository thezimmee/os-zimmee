  // set timezone to london
  date_default_timezone_set(substr(readlink('/etc/localtime'), strlen('/usr/share/zoneinfo/')));

  // read the time
  $time = date("H");
  if ($time &lt; "12") {
    echo "Good morning";
  } else if ($time &gt;= "12" &amp;&amp; $time &lt; "17") {
    echo "Good afternoon";
  } else if ($time &gt;= "17" &amp;&amp; $time &lt; "19") {
    echo "Good evening";
  } else {
    echo "Good night";
  }
